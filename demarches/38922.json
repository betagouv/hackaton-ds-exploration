              "MARAICHAGE",
              "OEILLETTE",
              "OIGNON : BULBILLES",
              "OIGNON : SEMIS",
              "OLEAGINEUX (colza, tournesol…)",
              "PATURE",
              "PEPINIERE",
              "PLANTE AROMATIQUE",
